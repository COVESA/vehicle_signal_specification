#
# Highlevel CARFIT vehicle signals.
#

StopAndGoTime:
  type: uint32
  unit: s
  description: Accumulated time spent under 10kph.

SurfaceStreetTime:
  type: uint32
  unit: s
  description: Accumulated time spent under and including 70kph.

HighwayTime:
  type: uint32
  unit: s
  description: Accumulated time spent over 70kph.
